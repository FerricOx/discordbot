{
  "name": "chaoticbacktalk",
  "version": "3.5.0",
  "description": "Discord Bot for the Chaotic Discord",
  "author": "Danude Sandstorm",
  "scripts": {
    "start": "NODE_ENV=development babel-watch src/bot.js --extensions \".js,.ts\"",
    "prebuild": "if [ -f err.log ]; then mv err.log err.old.log; fi; if [ -d src/db ]; then rm -rf src/db; fi",
    "build": "babel src -d build --copy-files --extensions \".js,.ts\"",
    "forever": "npm run build && forever -o out.log -e err.log start build/bot.js",
    "rebuild": "npm run stop; npm run forever",
    "stop": "forever stop build/bot.js",
    "clean": "rm -rf build/",
    "gitauto": "forever start node_modules/git-auto-pull/git-auto-pull.js ../../ \"npm run rebuild\"",
    "tsc": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaoticbackup/discordbot.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.4",
    "bufferutil": "^4.0.1",
    "discord.js": "^11.5.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "jquery": "^3.4.1",
    "jsdom": "^13.2.0",
    "lokijs": "^1.5.8",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "pdfjs-dist": "^2.2.228",
    "request-promise": "^4.2.5",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/jquery": "^3.3.31",
    "@types/jsdom": "^12.2.4",
    "@types/lokijs": "^1.5.2",
    "@types/node": "^12.12.12",
    "babel-eslint": "^10.0.3",
    "babel-watch": "^7.0.0",
    "eslint": "^6.7.0",
    "forever": "^1.0.0",
    "git-auto-pull": "^1.0.9",
    "typescript": "^3.7.2"
  }
}
