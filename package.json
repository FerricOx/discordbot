{
  "name": "chaoticbacktalk",
  "version": "3.5.0",
  "description": "Discord Bot for the Chaotic Discord",
  "author": "Danude Sandstorm",
  "scripts": {
    "start": "NODE_ENV=development babel-watch src/bot.ts --extensions \".js,.ts\"",
    "start:scan": "APP_ENV=test NODE_ENV=development babel-watch src/bot.ts --extensions \".js,.ts\"",
    "prebuild": "if [ -f out.log ]; then mv out.log out.old.log; fi; if [ -d src/db ]; then rm -rf src/db; fi; npm install",
    "build": "babel src -d build --delete-dir-on-start --copy-files --extensions \".js,.ts\"",
    "forever": "npm run build && forever -o out.log -e out.log start build/bot.js",
    "rebuild": "npm run stop; npm run forever",
    "stop": "forever stop build/bot.js",
    "clean": "rm -rf build/",
    "gitauto": "forever start node_modules/git-auto-pull/git-auto-pull.js ../../ \"npm run rebuild\"",
    "lint": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}'",
    "lint:fix": "tsc --noEmit && eslint '*/**/*.{js,ts,tsx}' --quiet --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chaoticbackup/discordbot.git"
  },
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@babel/runtime": "^7.7.4",
    "bufferutil": "^4.0.1",
    "discord-paginationembed": "^1.0.3",
    "discord.js": "^11.5.1",
    "express": "^4.17.1",
    "forever": "2.0.0",
    "fs-extra": "^8.1.0",
    "git-auto-pull": "^1.0.9",
    "jquery": "^3.4.1",
    "jsdom": "^13.2.0",
    "lokijs": "^1.5.8",
    "node-fetch": "^2.6.0",
    "path": "^0.12.7",
    "request-promise": "^4.2.5",
    "typescript": "^3.7.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/node": "^7.7.4",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-proposal-json-strings": "^7.7.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/plugin-syntax-import-meta": "^7.7.4",
    "@babel/plugin-transform-arrow-functions": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@babel/preset-typescript": "^7.7.4",
    "@types/express": "^4.17.2",
    "@types/fs-extra": "^8.0.1",
    "@types/jquery": "^3.3.31",
    "@types/jsdom": "^12.2.4",
    "@types/lokijs": "^1.5.2",
    "@types/node": "^13.5.0",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "babel-eslint": "^10.0.3",
    "babel-watch": "^7.0.0",
    "eslint": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-with-typescript": "^12.0.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1"
  }
}
